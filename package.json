{
  "name": "student-check-electron-angular",
  "version": "1.0.0",
  "description": "AOF Student Chcek-in Client Angular-Electron Branch",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "electron-rebuild": "electron-rebuild --pre-gyp-fix --force --module_dir . -w smartcard -e node_modules/electron-prebuilt",
    "postinstall": "npm run electron-rebuild",
    "rebuild": "npm rebuild --runtime=electron --target=1.4.3 --disturl=https://atom.io/download/atom-shell --abi=49",
    "package": "electron-packager . --asar=true"
  },
  "author": "codetector",
  "license": "ISC",
  "dependencies": {
    "angular-bootstrap-datetimepicker": "^1.1.3",
    "fs-extra": "^1.0.0",
    "jquery": "^3.1.1",
    "moment": "^2.17.0",
    "smartcard": "^1.0.16",
    "sqlite": "^2.2.3",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "electron-builder": "^5.10.5",
    "electron-packager": "^8.1.0",
    "electron-prebuilt": "^1.4.3",
    "electron-rebuild": "^1.1.5"
  }
}
